  <div class="row"><%= link_to " 新規登録", new_admin_live_house_path, class:"btn btn-secondary btn-sm mb-3" %></div>

  <div class="row">
  <table class="table table-striped">
   <thead class="thead-dark">
    <tr>
     <th>live house ID</th>
     <th>name</th>
     <th>税抜価格</th>
     <th>ジャンル</th>
     <th>ステータス</th>
    </tr>
   </thead>
   <thody>
    <% @live_houses.each do |live_house| %>
     <tr>
      <td><%= live_house.id %></td>
      <td><%= link_to live_house.name %></td>
      <td><%= live_house.price %></td>
      <td>地図</td>
      <td><%= live_house.is_active %></td>
    </tr>
    <% end %>
   </thody>
  </table>
  </div>